{
  "name": "react-lottie-light",
  "version": "1.2.7",
  "description": "lottie animation view for React - Fixed",
  "repository": {
    "type": "git",
    "url": "https://github.com/burgil/react-lottie-light"
  },
  "license": "MIT",
  "scripts": {
    "release": "git add package.json && git commit -m 'chore: bump version number' && standard-version --first-release && git push --follow-tags origin master && npm publish",
    "build": "npx babel ./src --out-dir ./dist",
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
    "testonly": "mocha --require .scripts/mocha_runner src/**/tests/**/*.js",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "start": "start-storybook -p 9010",
    "publish-storybook": "bash .scripts/publish_storybook.sh",
    "postpublish": "npm run publish-storybook"
  },
  "devDependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.3.1"
  },
  "dependencies": {
    "lottie-web": "^5.12.2"
  },
  "main": "dist/index.js"
}
